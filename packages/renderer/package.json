{
  "name": "@lunit/osd-react-renderer",
  "version": "1.2.0-alpha.3",
  "description": "OpenSeadragon viewer component for React using react-reconciler package.",
  "homepage": "https://github.com/lunit-io/osd-react-renderer#readme",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/types/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lunit-io/osd-react-renderer.git"
  },
  "keywords": [
    "openseadragon",
    "react",
    "reconciler",
    "deepzoom",
    "dzi"
  ],
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "compile": "microbundle --jsx React.createElement --tsconfig tsconfig.build.json",
    "clean": "rimraf ./dist tsconfig.tsbuildinfo",
    "test": "yarn jest",
    "test-watch": "jest"
  },
  "dependencies": {
    "react-reconciler": "^0.26.2"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/offscreencanvas": "^2019.7.1",
    "@types/openseadragon": "^3.0.7",
    "@types/react-reconciler": "^0.26.7",
    "jest": "^27.5.1",
    "microbundle": "^0.14.2",
    "openseadragon": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.1.5"
  },
  "peerDependencies": {
    "openseadragon": ">=2.4",
    "react": "^17.0.0  || ^18.2.0",
    "react-dom": "^17.0.0  || ^18.2.0"
  },
  "bugs": {
    "url": "https://github.com/lunit-io/osd-react-renderer/issues"
  }
}
